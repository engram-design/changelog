{
  "name": "nystudio107/changelog",
  "description": "A http://keepachangelog.com/ parser for the common developer",
  "authors": [
    {
      "name": "Bram Devries",
      "email": "bram@madewithlove.be"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "symfony/dom-crawler": "^3.0|^4.0|^5.0|^6.0",
    "symfony/css-selector": "^3.0|^4.0|^5.0|^6.0",
    "league/commonmark": "^1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~6"
  },
  "autoload": {
    "psr-4": {
      "Changelog\\": "src"
    }
  },
  "scripts": {
    "test": [
      "phpspec run --format=pretty"
    ]
  }
}
